{
  "name": "@hyperce/onblockit",
  "author": "Pukar Sharma<sharmapukar217@gmail.com>",
  "description": "OnBlockIt - Your LinkedIn Growth.",
  "keywords": [
    "LinkedIn Digital",
    "Landing Page",
    "Hyperce"
  ],
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "coverage": "vitest run --coverage",
    "typecheck": "svelte-kit sync && svelte-check",
    "format": "prettier -w src --ignore-path .gitignore",
    "lint": "eslint --fix src --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "formsnap": "^0.4.2",
    "svelte-french-toast": "^1.2.0",
    "sveltekit-superforms": "^1.13.2",
    "tailwind-merge": "^2.2.0",
    "workbox-window": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.3",
    "@egoist/tailwindcss-icons": "^1.7.1",
    "@iconify-json/flat-color-icons": "^1.1.10",
    "@iconify-json/heroicons": "^1.1.19",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/skill-icons": "^1.1.7",
    "@iconify/tailwind": "^0.1.4",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/enhanced-img": "^0.1.7",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "commitlint": "^18.4.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.0.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSameLine": true,
    "singleAttributePerLine": false,
    "plugins": [
      "prettier-plugin-svelte"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{ts(x),svelte}": [
      "eslint --fix --ignore-path .gitignore",
      "vitest"
    ],
    "*.{json,css,html}": "prettier -w --ignore-path .gitignore --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
